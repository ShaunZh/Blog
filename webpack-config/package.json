{
  "name": "webpack-config",
  "version": "1.0.0",
  "description": "webpack.config",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development ENV_TYPE=daily webpack-dev-server",
    "build": "cross-env NODE_ENV=production ENV_TYPE=daily webpack",
    "build:prod": "cross-env NODE_ENV=production ENV_TYPE=prod webpack",
    "generateAnalyzFile": "webpack --profile --json > ./dist/stats.json",
    "analyz": "npm run generateAnalyzFile && webpack-bundle-analyzer --port 8888 ./dist/stats.json",
    "dll": "webpack --config webpack.dll.config.js",
    "eslint": "eslint --ext .js,.ts,.tsx src",
    "eslint-fix": "eslint --fix --ext .js,.ts,.tsx src"
  },
